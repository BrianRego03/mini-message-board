<html>
    <head>
        <title><%= title %></title>
    </head>
    <body>
        <h1><%= title %></h1>
        <% for(let i=0; i< messages.length ; i++){ %>
            <div>
                <h2><%= messages[i].title %></h2>
                <p>From: <%= messages[i].user %></p>
                <p>Received: <%= messages[i].added %></p>
            </div>
        <% } %>    

    </body>
</html>